module strings

imports ds-signatures/Tiger-sig
imports dynamics/base
imports dynamics/natives
imports dynamics/numbers

signature
  constructors
    StringV : String -> V 
    
rules
 
  String(s) --> StringV(s')
  where
    unescapeS(s) => s'.
  
  Lt(StringV(i), StringV(j)) --> IntV(ltS(i, j)).
  
  Leq(StringV(i), StringV(j)) --> IntV(leqS(i, j)).